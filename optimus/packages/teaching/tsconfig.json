{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "lib": ["dom", "es2017"],
    "target": "esnext",
    "jsx": "preserve",
    "allowJs": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "isolatedModules": true,
    "strict": true
  },
  "include": ["client", "pages"]
}
